      @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;400;600&display=swap');
  
  
  /* SCROLL AQUI */
  
  
  ::-webkit-scrollbar {
  width: 0px;
  }
  
  ::-webkit-scrollbar-thumb {
  background-color: transparent;
  border-radius: 10px;
  }
  
  ::-webkit-scrollbar-track {
  background-color: transparent;
  }
  
  
  /* ESTRUTURA AQUI */
  
  
  body{
    margin:0;
    padding:0;
    color: #242424;
    font-family:'Poppins', sans-serif;
    word-wrap: break-word;
    overflow-wrap: break-word;
  }
  
  h1,h2,h3,h4,h5,h6{
    font-weight: 900;
    color: #000;
  }
  
  
  /* PARALLAX AQUI */
  
  
  .wrapper{
    width: 100%;
    margin: 0 auto;
    opacity: 0;
    transform: translateY(20px);
    animation: entrada-animacao 0.5s ease-in-out forwards;
  }
  
  @keyframes entrada-animacao {
      to {
          opacity: 1;
          transform: translateY(0);
      }
  }
  
  main{
    display: block;
    background-color: #FFF;
  }
  
  .container{
    width: 100%;
    max-width: 760px;
    margin: 0 auto;
    padding: 0 20px;
  }
  
  section.module p{
    margin-bottom: 40px;
  }
  
  section.module:last-child{
    margin-bottom: 0;
  }
  
  section.module h2{
    margin-bottom: 40px;
    font-size: 30px;
  }
  
  section.module.content{
   padding: 40px 0;
   background: #ece7d5; 
  }
  
  section.module.content:last-child{
    background-color: #010101;
  }
  
  section.module.parallax{
    display: flex;
    flex-direction: column;
    justify-content: center;
    height: 100vh !important;
    width: 100%;
    position: relative;
    overflow: hidden;
    background-position: 50% 50%;
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-size: cover;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
  }
  
  section.module.parallax:after{
    content: "";
    height: 100%;
    width: 100%;
    position: absolute;
    z-index: 8;
    background: -moz-linear-gradient(to bottom, rgba(0,0,0,0) 0, rgba(0,0,0,0) 40%, #000 100% );
    background: -webkit-linear-gradient(to bottom, rgba(0,0,0,0) 0, rgba(0,0,0,0) 40%, #000 100% );
    background: linear-gradient(to bottom, rgba(0,0,0,0) 0, rgba(0,0,0,0) 40%, #000 100% );
  }
  
  
  /* LISTA AQUI */
  
  
  .card {
    width: 100vh;
    height: 100%;
    background-color: #fff;
    padding: 10px 10px 10px 10px;
    border-radius: 10px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.4);
  }
  
  
  ol {
    margin: 5vh auto;
    padding: auto;
    font-size: 1em;
    list-style-type: decimal-leading-zero;
    display: flex;
    flex-direction:column;
  }
  
  li{
    font-size: 1.2em;
    margin: 10px;
    padding-bottom: 8px;
    border-bottom: 1px solid #000;
    display: flex;
    justify-content: space-between;
    transition: color 0.3s ease;
  }
  
  .card li span{
    margin-top: auto;
  }
  
  .card ol li a {
    font-size: 0.9em;
    text-decoration: none;
    color: #242424;
    background-color: #ece7d5;
    padding: 4px 10px;
    border-radius: 25px;
    transition: background-color 0.1s ease, transform 01s ease;
    display: inline-block;
    margin-top: auto;
  }
  
  .card li a:hover{
    background-color: #000;
    color: #FFF;
    transform:scale(1.1)
  }
  
  .card li:hover{
    color: #242424;
  }
  
  li:last-child{
    border: none;
  }
  
  section.module.parallax-1{
    background-image: url("/phpcurso.jpg");
  }
  
  section.module.parallax-2{
    background-image: url("/phphcurso2.png");
  }
  
  section.module.parallax-3{
    background-image: url("/foto.png");
  }
  
  section.module.parallax h1{
    color: #FFF;
    text-align: center;
    font-size: 78px;
    z-index: 50;
    text-transform: uppercase;
  }
  
  @media all and (max-width: 820px) {
    section.module h2 {
      font-size: 32px;
    }
    .module h2, p{
      padding: 0 8px;
    }
  
    .container{
      margin:0;
      padding:0;
    }
  
    section.module.parallax h1 {
      font-size: 36px;
    }
  }
    </STYle>

