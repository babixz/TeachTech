<% if (errors.length > 0) { %>
<div>
    <div class="alert-danger">
            <% errors.forEach(erro => { %>
                <%= erro %><br>
                <% }); %>
    </div>
</div>
<% } %>

<% if (success.length > 0) { %>
    <div>
        <div class="alert-sucesso">
                <% success.forEach(succes => { %>
                    <%= succes %><br>
                    <% }); %>
        </div>
    </div>
    <% } %>
    